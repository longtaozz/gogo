<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white">

    <include layout="@layout/base_head"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_16"
        android:layout_marginTop="@dimen/dp_18"
        android:layout_marginRight="@dimen/dp_15"
        android:orientation="vertical"
        >
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_14"
                    android:textColor="@color/color_484848"
                    android:text="城建号："/>
                <TextView
                    android:id="@+id/rich_scan_car_admitNumber"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_14"
                    android:textColor="@color/color_484848"
                    android:text=""/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_14"
                    android:textColor="@color/color_484848"
                    android:layout_marginTop="@dimen/dp_14"
                    android:text="车牌号码："/>
                <TextView
                    android:id="@+id/rich_scan_car_numberPlate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_14"
                    android:textColor="@color/color_484848"
                    android:layout_marginTop="@dimen/dp_14"
                    android:text=""/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_14"
                    android:textColor="@color/color_484848"
                    android:layout_marginTop="@dimen/dp_14"
                    android:text="SIM卡号："/>
                <TextView
                    android:id="@+id/rich_scan_car_simNumber"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_14"
                    android:textColor="@color/color_484848"
                    android:layout_marginTop="@dimen/dp_14"
                    android:text=""/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_14"
                    android:textColor="@color/color_484848"
                    android:layout_marginTop="@dimen/dp_14"
                    android:text="所属企业："/>
                <TextView
                    android:id="@+id/rich_scan_car_enterpriseName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_14"
                    android:textColor="@color/color_484848"
                    android:layout_marginTop="@dimen/dp_14"
                    android:text=""/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_14"
                    android:textColor="@color/color_484848"
                    android:layout_marginTop="@dimen/dp_14"
                    android:text="车辆所有人："/>
                <TextView
                    android:id="@+id/rich_scan_car_carOwnersName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_14"
                    android:textColor="@color/color_484848"
                    android:layout_marginTop="@dimen/dp_14"
                    android:text=""/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_14"
                    android:textColor="@color/color_484848"
                    android:layout_marginTop="@dimen/dp_14"
                    android:text="驾驶员姓名："/>
                <TextView
                    android:id="@+id/rich_scan_car_drivername"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_14"
                    android:textColor="@color/color_484848"
                    android:layout_marginTop="@dimen/dp_14"
                    android:text=""/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_14"
                    android:textColor="@color/color_484848"
                    android:layout_marginTop="@dimen/dp_14"
                    android:text="驾驶员联系电话："/>
                <TextView
                    android:id="@+id/rich_scan_car_drivertel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_14"
                    android:textColor="@color/color_484848"
                    android:layout_marginTop="@dimen/dp_14"
                    android:text=""/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_14"
                    android:textColor="@color/color_484848"
                    android:layout_marginTop="@dimen/dp_14"
                    android:text="驾驶证号："/>
                <TextView
                    android:id="@+id/rich_scan_car_drivingLicenseNumber"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_14"
                    android:textColor="@color/color_484848"
                    android:layout_marginTop="@dimen/dp_14"
                    android:text=""/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_14"
                    android:textColor="@color/color_484848"
                    android:layout_marginTop="@dimen/dp_14"
                    android:text="注册日期（行驶证）："/>
                <TextView
                    android:id="@+id/rich_scan_car_register"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_14"
                    android:textColor="@color/color_484848"
                    android:layout_marginTop="@dimen/dp_14"
                    android:text=""/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_14"
                    android:textColor="@color/color_484848"
                    android:layout_marginTop="@dimen/dp_14"
                    android:text="载重量（Kg）："/>
                <TextView
                    android:id="@+id/rich_scan_car_quality"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_14"
                    android:textColor="@color/color_484848"
                    android:layout_marginTop="@dimen/dp_14"
                    android:text=""/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_14"
                    android:textColor="@color/color_484848"
                    android:layout_marginTop="@dimen/dp_14"
                    android:text="车辆颜色："/>
                <TextView
                    android:id="@+id/rich_scan_car_color"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_14"
                    android:textColor="@color/color_484848"
                    android:layout_marginTop="@dimen/dp_14"
                    android:text=""/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_14"
                    android:textColor="@color/color_484848"
                    android:layout_marginTop="@dimen/dp_14"
                    android:text="密闭类型："/>
                <TextView
                    android:id="@+id/rich_scan_car_airtightype"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_14"
                    android:textColor="@color/color_484848"
                    android:layout_marginTop="@dimen/dp_14"
                    android:text=""/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_14"
                    android:textColor="@color/color_484848"
                    android:layout_marginTop="@dimen/dp_14"
                    android:text="从业监管信息："/>
                <TextView
                    android:id="@+id/rich_scan_car_certificate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_14"
                    android:textColor="@color/color_484848"
                    android:layout_marginTop="@dimen/dp_14"
                    android:text=""/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_14"
                    android:textColor="@color/color_484848"
                    android:layout_marginTop="@dimen/dp_14"
                    android:text="信用评价信息："/>
                <TextView
                    android:id="@+id/rich_scan_car_credit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_14"
                    android:textColor="@color/color_484848"
                    android:layout_marginTop="@dimen/dp_14"
                    android:text=""/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_20">
            <LinearLayout
                android:id="@+id/navicert"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:visibility="gone"
                android:gravity="center"
                android:background="@drawable/jn_zwt_history_button">
                <TextView
                    android:background="@color/white"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_20"
                    android:textSize="@dimen/text_14"
                    android:textColor="@color/color_1AAD19"
                    android:text="  准 运 证  "/>

            </LinearLayout>
            <LinearLayout
                android:layout_width="@dimen/dp_0"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:visibility="gone"
                android:gravity="center">
                <LinearLayout
                    android:id="@+id/interfacciami"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:background="@drawable/jn_zwt_history_button">
                    <TextView
                        android:background="@color/white"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dp_20"
                        android:textSize="@dimen/text_14"
                        android:textColor="@color/color_1AAD19"
                        android:text="违规记录"/>

                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/car_history"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:background="@drawable/jn_zwt_history_button">
                <TextView
                    android:background="@color/white"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_20"
                    android:textSize="@dimen/text_14"
                    android:textColor="@color/color_1AAD19"
                    android:text="历史轨迹"/>

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

</LinearLayout>