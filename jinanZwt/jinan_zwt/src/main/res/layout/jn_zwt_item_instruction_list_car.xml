<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_26"
    android:background="@color/white"
    android:gravity="center_vertical"
    android:orientation="horizontal">
    <ImageView
        android:id="@+id/instructions_list_box"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="@dimen/dp_8"
        android:layout_marginLeft="@dimen/dp_8"
        android:src="@drawable/jn_zwt_selection_img" />
    <TextView
        android:id="@+id/instructions_list_carnumber"
        android:layout_width="@dimen/dp_0"
        android:gravity="center"
        android:layout_weight="1"
        android:layout_height="wrap_content"
        android:textSize="@dimen/text_14"
        android:textColor="@color/color_4a4a4a"
        android:text="鲁AV9283"
        />
    <TextView
        android:id="@+id/instructions_list_sim"
        android:layout_width="@dimen/dp_0"
        android:gravity="center"
        android:layout_weight="1.4"
        android:layout_height="wrap_content"
        android:textSize="@dimen/text_14"
        android:layout_marginLeft="@dimen/text_11"
        android:textColor="@color/color_4a4a4a"
        android:text="64874465761"
        />
    <TextView
        android:id="@+id/instructions_list_company"
        android:layout_width="@dimen/dp_0"
        android:layout_weight="1.8"
        android:layout_height="wrap_content"
        android:textSize="@dimen/text_14"
        android:gravity="center"
        android:layout_marginLeft="@dimen/text_11"
        android:textColor="@color/color_4a4a4a"
        android:text="XXXXXXXXXX公司"
        />
    <View
        android:layout_width="@dimen/dp_15"
        android:layout_height="wrap_content"></View>
</LinearLayout>